rules:
  no-new: 0
  no-unused-vars:
    - 2
    - vars: all
      args: none
  indent:
    - 2
    - 2
  quotes:
    - 2
    - single
  linebreak-style:
    - 2
    - unix
  semi:
    - 2
    - always
env:
  es6: true
  node: true
  browser: true
globals:
  chrome: true

parserOptions:
  sourceType: module
